#config.json文件中所有备注都需要删除，否则报错

{
  "channel_type": "wechatcom_app",
  "single_chat_prefix": [""],  
  "wechatcom_corp_id": "xxx", # 企业微信corp_id
  "wechatcomapp_token": "xx", # 企业微信应用token,创建   -> 应用 -> 应用详情 -> 应用ID
  "wechatcomapp_secret": "Psxxxxxx", # 企业微信应用密钥，企业微信，后台配置入口：首页agent ID下面就是，需要点击查看，企业微信端确认后就可以看到 
  "wechatcomapp_agent_id": "1000xxx", # 企业微信应用id，创建应用后，首页AgentID
  "wechatcomapp_aes_key": "U2Axx", # 企业微信应用aes_key，企业微信后台：应用管理->API接收消息 里面和token一起生成，保存用于回填
  "wechatcomapp_port": 9898,
  "open_ai_api_key": "fastgpt-xx", #fastgpt应用- api服务
  "open_ai_api_base": "http://ip:3100/api/v1",  #fastgpt应用api地址，记得加V
  "model": "gpt-3.5-turbo", #建议填写，使用openai模块，避免报错
  "text_to_image": "dall-e-2",
  "voice_to_text": "openai",
  "text_to_voice": "openai",
  "proxy": "",
  "hot_reload": false,
  "single_chat_reply_prefix": "[小胰宝] ",
  "group_chat_prefix": [
    "@小胰宝"
  ],
  "group_name_white_list": [
    "ChatGPT测试群",
    "ChatGPT测试群2"
  ],
  "image_create_prefix": [
    "x画"
  ],
  "speech_recognition": false,
  "group_speech_recognition": false,
  "voice_reply_voice": false,
  "conversation_max_tokens": 2500,
  "expires_in_seconds": 3600,
  "character_desc": "你是基于大语言模型的AI智能助手，旨在回答并解决人们的任何问题，并且可以使用多种语言与人交流。",
  "temperature": 0.7,
  "subscribe_msg": "感谢您的关注！\n这里是AI智能助手，可以自由对话。\n支持语音对话。\n支持图片输入。\n支持图片输出，画字开头的消息将按要求创作图片。\n支持tool、角色扮演和文字冒险等丰富的插件。\n输入{trigger_prefix}#help 查看详细指令。",
  "use_linkai": false,
  "linkai_api_key": "",
  "linkai_app_code": ""
}
